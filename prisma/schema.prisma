generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model category {
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  title     String @unique  @db.VarChar(255)
  description   String?
  status Boolean @default(true)
}

model brand {
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  title     String @unique  @db.VarChar(255)
  description   String?
  status Boolean @default(true)
}